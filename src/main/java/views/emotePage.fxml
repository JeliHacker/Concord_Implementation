<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<BorderPane prefHeight="238.0" prefWidth="154.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="views.AddEmoteController">
   <center>
      <VBox alignment="TOP_CENTER" prefHeight="200.0" prefWidth="100.0" style="-fx-spacing: 8;" BorderPane.alignment="CENTER">
         <children>
            <Label text="Select Emoji" textAlignment="CENTER" />
            <HBox alignment="TOP_CENTER" prefHeight="100.0" prefWidth="200.0">
               <children>
                  <ImageView fitHeight="40.0" fitWidth="40.0" pickOnBounds="true" preserveRatio="true" />
                  <VBox alignment="TOP_CENTER" prefHeight="200.0" prefWidth="100.0">
                     <children>
                        <Label text="Image URL:" />
                        <TextField />
                     </children>
                  </VBox>
               </children>
            </HBox>
            <Button fx:id="addEmojiButton" mnemonicParsing="false" onAction="#onClickAddEmoji" text="Add Emoji" />
         </children>
      </VBox>
   </center>
   <top>
      <HBox alignment="CENTER_LEFT" prefHeight="33.0" prefWidth="154.0" BorderPane.alignment="CENTER">
         <children>
            <Button fx:id="Cancel_B" mnemonicParsing="false" onAction="#onClickCancel" text="Cancel" />
         </children>
      </HBox>
   </top>
</BorderPane>
